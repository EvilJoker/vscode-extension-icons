{
  "name": "icons",
  "displayName": "vscode-buildin-icons",
  "description": "easy to find VSCode build-in icons ",
  "icon" : "src/resources/vscode.png",
  "version": "0.0.1.test",
  "publisher": "AshtonSun",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvilJoker/vscode-extension-icons.git"
  },
  "bugs": {
    "url": "https://github.com/EvilJoker/vscode-extension-icons/issues"
  },
  "homepage": "https://github.com/EvilJoker/vscode-extension-icons",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vsocde-icons",
          "title": "icons",
          "icon": "$(jersey)"
        }
      ]
    },
    "commands": [
      {
        "command": "icons.find",
        "title": "find icon whthin VSCode build-in icons",
        "category": "Icons "
      }
    ],
    "menus": {},
    "views": {
      "vsocde-icons": [
        {
          "type": "webview",
          "id": "icons.iconsView",
          "name": "icons",
          "title": "icons"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
